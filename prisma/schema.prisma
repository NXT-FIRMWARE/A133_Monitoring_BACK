// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Camera {
  id           Int    @id @default(autoincrement())
  cameraName   String @unique()
  rtsp         String
  ip           String @unique()
  path         String
  capture_time Int
  http_server  String
  ftp_server   String
  ftp_username String
  ftp_password String
}
